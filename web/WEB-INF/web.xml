<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <filter>
        <filter-name>checkeLogin</filter-name>
        <filter-class>cerong.erp.filter.CheckLoginFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>checkeLogin</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <session-config>
        <session-timeout>60</session-timeout>
    </session-config>
    <listener>
        <listener-class>cerong.erp.TimerListener.MoldListener</listener-class>
    </listener>
    <listener>
        <listener-class>cerong.erp.TimerListener.InitListener7</listener-class>
    </listener>
    <servlet>
        <servlet-name>ItCaseIdServlet</servlet-name>
        <servlet-class>cerong.erp.servlet.ItCaseIdServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ItCaseIdServlet</servlet-name>
        <url-pattern>/ItCaseIdServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EmployeeServlet</servlet-name>
        <servlet-class>cerong.erp.servlet.EmployeeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EmployeeServlet</servlet-name>
        <url-pattern>/EmployeeServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CustomerServlet</servlet-name>
        <servlet-class>cerong.erp.servlet.CustomerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CustomerServlet</servlet-name>
        <url-pattern>/CustomerServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AbstractHelpServlet</servlet-name>
        <servlet-class>cerong.erp.servlet.AbstractHelpServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AbstractHelpServlet</servlet-name>
        <url-pattern>/helpServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CheckUpServlet</servlet-name>
        <servlet-class>cerong.erp.servlet.CheckUpServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckUpServlet</servlet-name>
        <url-pattern>/CheckUpServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletDownload</servlet-name>
        <servlet-class>cerong.erp.servlet.ServletDownload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletDownload</servlet-name>
        <url-pattern>/download</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletDownload1</servlet-name>
        <servlet-class>cerong.erp.servlet.ServletDownload1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletDownload1</servlet-name>
        <url-pattern>/download1</url-pattern>
    </servlet-mapping>
</web-app>